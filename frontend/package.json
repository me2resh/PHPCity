{
  "name": "phpcity-frontend",
  "version": "2.0.0",
  "description": "City metaphor visualization of PHP projects - Frontend",
  "main": "./dist/main.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "serve": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "clean": "rimraf dist",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adrianhuna/PHPCity"
  },
  "keywords": [
    "visualization",
    "PHP",
    "project",
    "structure",
    "three.js",
    "typescript",
    "3d"
  ],
  "author": "Adrian Huna",
  "license": "MIT",
  "dependencies": {
    "three": "^0.158.0",
    "@types/three": "^0.158.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-legacy": "^5.0.0",
    "eslint": "^8.57.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.4.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0"
  }
}